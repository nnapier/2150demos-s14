
public class Pet
{
	// Instance variables
	private String name;
	private String breed;
	private String color;
	private int numLegs;
	private double weight;
	
	// Constructors
	public Pet()
	{
		this("Pet, "mutt", "gray", 4, 4.5);
					
	}
	
	public Pet(String name, String breed, String color,
			   int numLegs, double weight)
	{
		this.name = name;
		this.breed = breed;
		this.color = color;
		this.numLegs = numLegs;
		this.weight = weight;
	}

}
