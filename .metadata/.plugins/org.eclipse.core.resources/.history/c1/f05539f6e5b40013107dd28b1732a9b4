import java.util.Stack;


public class StackExample
{

	/**
	 * @param args
	 */
	public static void main(String[] args)
	{
		Stack<String> names = new Stack<String>();
		names.add("Dr.Napier");
		names.add("Allen");
		names.add("Dr.Seuss");
		names.add("Willy Wonka");
		names.add("Big Bird");
		while(!names.isEmpty())
		{
			System.out.println("The top of the stack is: " + names.pop());
		}
	}

}
