/**
 * @author nnapier
 *
 */
public class Cat
{
	private String name;
	private String breed;
	private String color;
	private double weight;
	private boolean isVaccinated;
	
	// Constructors
	public Cat()
	{
		name = "Garfield";
		breed = "mutt";
		color = "orange";
		weight = 4.5;
		isVaccinated = true;
	}

}
