public class Cat extends Pet
{
	// Instance variable
	public enum LitterType { SCOOPABLE, REGULAR, CLUMP_FREE } 
	
	private LitterType litterType;
	
	// no-arg Constructor
	public Cat()
	{
		super();
		litterType = LitterType.REGULAR;
	}

}
