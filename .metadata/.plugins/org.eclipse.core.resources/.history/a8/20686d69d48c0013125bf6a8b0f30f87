
public class Person
{
	// instance variables
	private String name;
	private String city;
	
	// Constructors
	public Person(String name, String city)
	{
		this.name = name;
		this.city = city;
	}
	
	public Person()
	{
		this("Nannette", "Atlanta");
		/* Instead of doing this
		name = "Nannette";
		city = "Atlanta";
		*/
	}

}
